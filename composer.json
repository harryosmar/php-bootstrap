{
    "name": "harryosmar/php-bootstrap",
    "description": "php bootstrapping for simple web app",
    "type": "project",
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "authors": [
        {
            "name": "Harry Sitohang",
            "email": "harry@olx.co.id"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "league/route": "^3.0",
        "symfony/console": "^3.3",
        "league/fractal": "^0.17.0",
        "phpcollection/phpcollection": "^0.5.0",
        "zendframework/zend-diactoros": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "PhpBootstrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
